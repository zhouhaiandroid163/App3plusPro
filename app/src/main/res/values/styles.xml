<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.




    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.




        -->
    </style>

    <!-- 主题色主题 -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/public_theme_index_title</item>
        <item name="colorPrimaryDark">@color/public_theme_index_state</item>
        <item name="colorAccent">@color/top_bar_base</item>
    </style>

    <!-- 白色主题 -->
    <style name="AppThemeWhite" parent="Theme.AppCompat.Light.DarkActionBar">
        <!--标题栏颜色-->
        <item name="colorPrimary">@color/top_bar_base</item>
        <!--状态栏颜色-->
        <item name="colorPrimaryDark">@color/top_bar_base</item>
        <!--控制各个控件被选中时的颜色-->
        <item name="colorAccent">@color/public_theme_index_enable</item>
    </style>

    <!-- 白色主题 -->
    <style name="AppTheme1" parent="Theme.AppCompat.Light">
        <!--标题栏颜色-->
